<!DOCTYPE html>
<html lang="en">
<head>
   <%- include('partials/head.ejs')%>
</head>
<body>
    <%- include('partials/header.ejs') %>

    <section class="container">
        <%- include('partials/navBar.ejs')%>
        <article class="content">
            <h1>POSTS</h1>

                <div class="posts">
                    <% posts.forEach((item)=>{  %>
                        <div class="post">
                            <h3> <%= item.title %> </h3>
                            <p> <%= item.text %> </p>
                        </div>
                    <% }) %>
                </div>
        </article>
    </section>

    <%- include('partials/footer.ejs')%>
    <%- include('partials/foot.ejs') %>
</body>
</html>